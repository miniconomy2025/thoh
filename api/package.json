{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/main.ts",
    "start": "npm run typeorm -- migration:run --dataSource src/domain/shared/data-source.ts && ts-node src/main.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:queue": "ts-node src/test/queue-test.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/infrastructure/persistence/postgres/migrations/third --dataSource src/domain/shared/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run --dataSource src/domain/shared/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert --dataSource src/domain/shared/data-source.ts",
    "seed": "ts-node src/domain/market/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.844.0",
    "@prisma/client": "^6.11.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.25",
    "undici": "^7.11.0",
    "vitest": "^3.2.4",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
